<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Groups for sharing data with System Extension -->
    <key>com.apple.security.application-groups</key>
    <array>
        <string>group.com.voyage.mac</string>
    </array>
    
    <!-- Network Extension capabilities -->
    <key>com.apple.developer.networking.networkextension</key>
    <array>
        <string>packet-tunnel-provider</string>
    </array>
    
    <!-- System Extension activation -->
    <key>com.apple.developer.system-extension.install</key>
    <true/>
    
    <!-- Hardened Runtime -->
    <key>com.apple.security.app-sandbox</key>
    <true/>
    
    <!-- Network client access -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Network server access (for local IPC) -->
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- File access for configuration -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
</dict>
</plist>
